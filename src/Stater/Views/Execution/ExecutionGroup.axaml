<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Stater.Views.Execution.ExecutionGroup">
    <DockPanel LastChildFill="True">
        <Border Background="Beige" Width="200" BorderBrush="Black" BorderThickness="1" DockPanel.Dock="Left">
            <TextBlock Foreground="Black">Variables</TextBlock>
        </Border>
        <Border Background="SkyBlue" BorderBrush="Black" BorderThickness="1" DockPanel.Dock="Right">
            <TextBlock Foreground="Black">Execution Control</TextBlock>
        </Border>
    </DockPanel>
</UserControl>